
{
  "name": "password-leaker-api",
  "version": "1.0.0",
  "description": "Production-ready Cloudflare Worker API for Password Leaker PRO",
  "main": "api.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:staging": "wrangler deploy --env staging", 
    "deploy:prod": "wrangler deploy --env production",
    "deploy": "wrangler deploy --env production",
    "test": "node test/api.test.js",
    "logs": "wrangler tail",
    "logs:prod": "wrangler tail --env production"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "hono": "^4.8.2"
  },
  "devDependencies": {
    "wrangler": "^3.78.0"
  },
  "keywords": [
    "cloudflare-workers",
    "password-security", 
    "api",
    "serverless",
    "hono"
  ],
  "author": "Denti Systems",
  "license": "MIT"
}
